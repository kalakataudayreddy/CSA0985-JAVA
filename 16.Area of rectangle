import java.util.Scanner;
class Area
{
	int res=0;
	Area(int x,int y)
	{
	 res=x*y;
	}
}
class mul
{
public static void main(String[] arg)
{
	int a,b;
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter length:");
	a=sc.nextInt();
	
	System.out.println("Enter breadth:");
	b=sc.nextInt();
	Area c=new Area(a,b);
	System.out.println("Area of rectangle is:"+c.res);
}
}
