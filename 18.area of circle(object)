import java.io.*;
import java.util.*;
class area{
int r;
float pi=3.14f,result;
void display(int r)
{
r=r;
result=pi*r*r;
System.out.println(result);
}
}
class circle
{
public static void main(String[] args)
{
area a1=new area();
area a2=new area();
Scanner input= new Scanner(System.in);
System.out.println("the radius of the circle:");
a1.r=input.nextInt();
a1.display(a1.r);
}
}
